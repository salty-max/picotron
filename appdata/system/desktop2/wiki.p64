include("lib/json.lua")
-- (https://github.com/rxi/json.lua)
-- incredible lightweight json implementation

--[[

	Picotron Wiki Viewer
	- written by scrapSavage
	- maintained by Picotron Software Developers
	(https://github.com/Picotron-Software-Developers)
	
	Feel free to make pull requests on our wiki
	repository.

]]--

local version = "0.4" -- used to prevent crashes

local wiki_repository = "https://raw.githubusercontent.com/Picotron-Software-Developers/Picotron-Wiki/main/"
local page = "Landing"

local page_json = {}

local page_history = {}
local back_history = {}

window({
	width=300,height=200,
	title="Picotron Wiki"
})

local background_width = 256
local padding = 8

local gui
local background
local scrollbar
local action_bar

function lerp(a,b,t)
	return a*(1-t)+b*t
end

function txtw(txt)
	return #txt*6
end
function txth(txt)
	local a, b = txt:gsub("\n","\n")
	return (b+1)*12
	--local _w,h=print(txt,0,0)
	--return h
end

menuitem({
	id=0,
	label="Return to landing page",
	action=(function()
		page="Landing"
		refresh()
	end)
})

function _init()
	gui = create_gui()
	background = gui:attach({
		x=0,y=0,width=background_width,height=32,
	})
	scrollbar = gui:attach_scrollbars({
		autohide=true
	})
	local action_buttons = {
			{spr=0,action=(function() if #page_history==1 then notify("You haven't gone anywhere past this.") return end add(back_history,page_history[#page_history]) deli(page_history, #page_history) page=page_history[#page_history] refresh() deli(page_history,#page_history) end)}, -- Back button
			{spr=1,action=(function() page="Landing" refresh() end)}, -- Home button
			{spr=2,action=(function() if #back_history==0 then return end page=back_history[#back_history] refresh() deli(back_history,#back_history) end)}, -- Forward button
			{spr=6,action=(function() create_process("popups/authors.lua",{argv={page_json.metadata.authors}}) end)}, -- Authors button
			{spr=3,action=(function() create_process("popups/editor.lua",{argv={page_json,pid()}}) end)}
	}
	action_bar = gui:attach({
		x=-12,y=4,width=#action_buttons*8+1,height=11,justify="right",
		draw=(function(self)
			rectfill(0,0,1000,1000,0)
		end)
	})
	for i, el in pairs(action_buttons) do
	local b = action_bar:attach({
		x=(i-1)*8+1,y=2,width=8,height=8,
		off_y=0,spr=el.spr,action=el.action,
		draw=(function(self)spr(self.spr,0,self.off_y) end),
		tap=(function(self)self.action() self.off_y=0 end),
		click=(function(self)self.off_y=1 end)
	})
	end
	refresh()
end

function refresh()
	local data
	local result
	local dontuse
	local co = cocreate(fetch)
	while costatus(co)!="dead" do
		dontuse, result = coresume(co,wiki_repository..page:lower()..".json")
	end
	data = result
	if data == "404: Not Found" then
		data = fetch(wiki_repository.."404.json")
	end
	json.decode(data)
end

function local_refresh()
	local data = fetch("downloaded_pages/"..page)
	json.decode(data)
end

local warn_txt = "tHIS ui WAS NOT DESIGNED FOR THIS WIDTH."
local warn_x = -txtw(warn_txt)-2

local attached_elements = {}

function _draw()
	window({title="Picotron Wiki - "..page})
	cls(21)
	poke(0x4000, get(fetch("/system/fonts/lil_mono.font")))
	background.x=get_display():width()/2-background_width/2
	rectfill(background.x,0,background.width+background.x-1,1000,5)
	gui:update_all()
	action_bar.x=scrollbar.hidden and -4 or -12
	gui:draw_all()
	local warn = get_display():width()<background_width	
	poke(0x4000, get(fetch("/system/fonts/p8.font")))
	warn_x=math.ceil(lerp(warn_x,warn and 3 or -txtw(warn_txt)-2,0.5))
	print("\#0"..warn_txt,warn_x,get_display():height()-8,7)
end

function remove_elements()
	for e in all(attached_elements) do
		background:detach(e)
		if e.get_text then -- for text editors
			background:detach(e.parent)
		end
	end
	background.height=0
end

function draw_page_from_data(data)
	remove_elements()
	attached_elements = {}
	if page_history[#page_history]!=page then
		page_history[#page_history+1]=page
	end

	local draw_height = 32
	
	-- reset scrolling, feels hacky
	-- but forcing the bar_y on the scrollbar
	-- doesnt' work :/
	scrollbar.parent:mousewheel({wheel_y=-1000})

	if tonum(data.metadata.version) > tonum(version) and data.metadata.page_title!="404" then
		local display = background:attach({
		x=padding,y=padding,width_rel=1,height=64,
		label="This page is for wiki viewer version "..data.metadata.version.."\nYou are using version "..version,draw=(function(self)
			print(self.label,0,0,7)
		end)
		})
		add(attached_elements,display)
		return
	end	

	local _title_text = background:attach({
		x=padding,y=padding,width_rel=1,height=64,
		label=tostr(data.metadata.page_title),draw=(function(self)
			print("\^w\^t"..self.label,0,0,7)
		end)
	})
	add(attached_elements,_title_text)
	local element_case = {
		["label"] = (function(element)
			local el = background:attach({
				x=padding,y=draw_height,width_rel=1,height=txth(element.text),
				label=tostr(element.text),draw=function(self)
					print(self.label,0,0,7)
				end
			})
			draw_height+=txth(element.text)+padding/2
			add(attached_elements,el)
		end),
		["snippet"] = (function(element)
			local snippet = background:attach_text_editor({
				x=padding,y=draw_height,
				width=background_width-padding*2,
				height=txth(element.text)+2,
				syntax_highlighting=true,
				markup=false,
				embed_pods=true,
				has_search=false
			})
			draw_height+=txth(element.text)+padding
			snippet:set_text(element.text)
			add(attached_elements,snippet)
		end),
		["dir_ref"]=(function(element)
			local el = background:attach_button({
				x=padding,y=draw_height,width=txtw(element.text)+padding,
				height=txth(element.text)+2,label=element.text,
				tap=(function()
					create_process("/system/apps/filenav.p64", {argv={element.ref}})
				end)
			})
			draw_height+=txth(element.text)+padding
			add(attached_elements,el)
		end),
		["wiki_ref"]=(function(element)
			local el = background:attach_button({
				x=padding,y=draw_height,width=txtw(element.text)+padding,
				height=txth(element.text)+2,label=element.text,
				tap=(function()
					page = element.ref
					refresh()
					back_history = {}
				end)
			})
			draw_height+=txth(element.text)+padding
			add(attached_elements,el)
		end),
		["pod_image"]=(function(element)
			local sd = unpod(element.data)
			local el = background:attach({
				x=padding,y=draw_height,
				width=sd:width(),
				height=sd:height(),
				sprite_data = sd,
				draw=(function(self)
					rectfill(0,0,999,999,0)
					spr(self.sprite_data,0,0)
				end)
			})
			draw_height+=sd:height()+padding
			add(attached_elements,el)
		end),
		["demo"] = (function(element)
			local snippet = background:attach_text_editor({
				x=padding,y=draw_height,
				width=background_width-padding*2,
				height=txth(element.text)+2+padding*2,
				syntax_highlighting=true,
				markup=false,
				embed_pods=true,
				has_search=false
			})
			snippet:set_text(element.text)
			snippet:attach_button({
				x=background_width-74-padding,
				y=txth(element.text)+4,width=64,height=13,
				label="Run Demo",tap=(function()
					create_process("popups/demo.lua",{argv={table.concat(snippet:get_text(),"\n")}})
				end)
			})
			draw_height+=txth(element.text)+padding*3+2
			add(attached_elements,snippet)
		end),
		["note"]=(function(element)
			local el = background:attach({
				x=padding,y=draw_height,
				width=background_width-padding*2,
				height=txth(element.text.."\n")+padding+2,
				my_txt=element.text,draw=(function(self)
					rectfill(0,0,1000,1000,12)
					rectfill(0,0,3,1000,28)
					?"Note:",12,padding,7
					print(self.my_txt,16,18)
				end)
			})
			draw_height+=el.height+padding
			add(attached_elements,el)
		end),
		["warning"]=(function(element)
			local el = background:attach({
				x=padding,y=draw_height,
				width=background_width-padding*2,
				height=txth(element.text.."\n")+padding+2,
				my_txt=element.text,draw=(function(self)
					rectfill(0,0,1000,1000,9)
					rectfill(0,0,3,1000,10)
					?"Warning!",12,padding,7
					print(self.my_txt,16,18)
				end)
			})
			draw_height+=el.height+padding
			add(attached_elements,el)
		end)
	}
	
	for element in all(data.elements) do
		element_case[element.type](element)
	end
	background.height=draw_height
end

--dropped in straight from SLATE
on_event("drop_items",function(msg)
	for i=1,#msg.items do
		local item = msg.items[i]
		if item.pod_type == "file_reference" then
			if item.attrib == "file" then
				local data = fetch(item.fullpath)
				page="Local Page"
				draw_page_from_data(json.decode(data))
			else
				create_process("/system/apps/filenav.p64", {argv={item.fullpath}})
				notify(item.filename.." is a folder.")
			end
		else
			notify("Couldn't open "..item.filename)
		end
	end
end)

on_event("reload_page",function(msg)
	draw_page_from_data(msg.data)
	page_json = msg.data
	gui:update_all()
	gui:draw_all()
end)
