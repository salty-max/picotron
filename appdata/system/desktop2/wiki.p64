--[[pod_format="raw",created="2024-03-24 14:12:58",author="scrapSavage",icon=userdata("u8",16,16,""),modified="2024-03-26 23:59:05",notes="Allows easy editing of wiki pages",revision=480,stored="2024-03-24 14:28:16",title="Wiki Page Editor",version="0.1"]]
--[[

		Picotron Wiki Page Editor
		written by scrapSavage

]]--

window({width=152,height=152,title="Picotron Wiki Editor"})
include("../lib/json.lua")
local json_data = env().argv[1]
local viewer_pid = env().argv[2]
local gui

local button_gfx = {
	push=unpod("b64:bHo0AB0AAAAbAAAA8AxweHUAQyAICASQJzAXABcQJwAnEBcAFzAn8AM="),
	save=unpod("b64:bHo0ACUAAAAjAAAA8BRweHUAQyAICAQHACcgBwAnAAcAB0AHAGcABwBHAAcARwBngA=="),
	meta=unpod("b64:bHo0ABwAAAAmAAAA_wNweHUAQyAICAQAFwAXEBcgFwAEAFAQFwAXkA=="),
	folder=unpod("b64:bHo0ABwAAAAdAAAA0XB4dQBDIAgIBEAXAGcCALAHQAcABwAnAAfwAQ==")
}

local background_width = 125
local padding = 8

local selected_element = 1

local attached_elements = {}
local action_bar

function txth(txt)
	local a, b = txt:gsub("\n","\n")
	return (b+1)*11
end

function _init()
	gui = create_gui()
	background = gui:attach({
		x=0,y=0,width=background_width,height=8,
	})
	scrollbar = gui:attach_scrollbars({
		autohide=true
	})
	local action_buttons = {
		{spr=button_gfx.save,action=(function()
			local title = json_data.metadata.page_title:gsub("%W","")
			local save_path = (pwd():sub(1,#pwd()-6)).."/my_pages/"
			local file_name = title..".json"
			store(save_path..file_name,json.encode(json_data),{})
			notify("Saved page to "..save_path..file_name)
			end)}, -- Save
		{spr=button_gfx.folder,action=(function() 
			create_process("/system/apps/filenav.p64", {argv={(pwd():sub(1,#pwd()-6)).."/my_pages/"}})
		end)}, -- Folder
		{spr=button_gfx.meta,action=(function() create_process("editor/edit_attributes.lua",{argv={pid(),false,json_data.metadata,-1}}) end)}, -- Meta
		{spr=button_gfx.push,action=(function() send_message(viewer_pid,{event="reload_page",data=json_data}) end)} -- Push to viewer
	}
	action_bar = gui:attach({
		x=2,y=2,width=9,height=#action_buttons*9,
		draw=(function(self)
			rectfill(0,0,1000,1000,0)
		end)
	})
	for i, el in pairs(action_buttons) do
	local b = action_bar:attach({
		x=1,y=(i-1)*9+1,width=8,height=8,
		off_y=0,spr=el.spr,action=el.action,
		draw=(function(self)spr(self.spr,0,self.off_y) end),
		tap=(function(self)self.action() self.off_y=0 end),
		click=(function(self)self.off_y=1 end)
	})
	end
	construct_from_json()
end

function _draw()
	palt(0,false)
	cls(16)
	poke(0x4000,get(fetch("/system/fonts/lil_mono.font")))
	background.x=get_display():width()/2-background_width/2
	rectfill(background.x,0,background.width+background.x-1,1000,1)
	background.height = #json_data.elements*12+8
	gui:update_all()
	poke(0x4000,get(fetch("/system/fonts/p8.font")))
	gui:draw_all()
end

function construct_from_json()
	for element in all(attached_elements) do
		background:detach(element)
	end
	attached_elements = {}
	for i, element in pairs(json_data.elements) do
		add_element(element,i)
	end
	local new_button = background:attach({
		x=background_width/2-22,y=#attached_elements*11+2,
		width=47,height=7,draw=(function(self)
			rectfill(0,0,999,999,16)
			?"new element",2,1,1
		end),cursor="pointer",tap=(function(self)
			create_process("editor/edit_attributes.lua",{argv={pid(),true}})
		end)
	})
	add(attached_elements,new_button)
end

function add_element(element,i)
	local el = background:attach({
		cursor="pointer",
		x=2,y=2+(i-1)*11,width=background_width-padding/2,
		height=9,my_element=element,my_idx=i,
		draw=(function(self)
			rectfill(0,0,999,999,self.my_idx==selected_element and 12 or 16)
			?self.my_element.type,2,2,self.my_idx==selected_element and 16 or 1
			if self.my_idx == selected_element then
				?"\^iedit",self.width-54,2,16
				?"\^iup",self.width-35,2,16
				?"\^idown",self.width-24,2,16
				?"\^ix",self.width-5,2,16
			end
		end),
		tap=(function(self)
			local mx = mouse()-background.x-self.x
			if selected_element != self.my_idx then 
				selected_element=self.my_idx
				return
			end
			if mx>=114 and mx<=119 then -- delete
				deli(json_data.elements,self.my_idx)
				if self.my_idx>#json_data.elements then
					selected_element=#json_data
				end
				construct_from_json()
			end
			if mx>=95 and mx<=111 then -- down
				if self.my_idx==#json_data.elements then return end
				local old_data = json_data.elements[self.my_idx+1]
				json_data.elements[self.my_idx+1]=json_data.elements[self.my_idx]
				json_data.elements[self.my_idx]=old_data
				construct_from_json()
			end
			if mx>=84 and mx<=93 then -- up
				if self.my_idx==1 then return end
				local old_data = json_data.elements[self.my_idx-1]
				json_data.elements[self.my_idx-1]=json_data.elements[self.my_idx]
				json_data.elements[self.my_idx]=old_data
				construct_from_json()
			end
			if mx>=65 and mx<=82 then -- edit
				create_process("editor/edit_attributes.lua",{argv={pid(),false,self.my_element,self.my_idx}})
			end
		end)
	})
	add(attached_elements,el)
end

on_event("update_at",function(msg)
	json_data.elements[msg.data.idx]=msg.data.element
	construct_from_json()
end)

on_event("update_meta",function(msg)
	json_data.metadata=msg.data
end)

on_event("attach_object",function(msg)
	add(json_data.elements,msg.el)
	construct_from_json()
end)
