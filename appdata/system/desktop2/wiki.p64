--[[pod_format="raw",created="2024-03-25 00:48:01",modified="2024-04-08 14:27:16",revision=241]]
--[[

	Edit attributes of a Wiki Element
	written by scrapSavage
	
	What are you doing here?

]]--

local editor_pid = env().argv[1]
local creating = env().argv[2]
local element = nil
local element_idx = -1

if not creating then
	element = env().argv[3]
	element_idx = env().argv[4]
end

local gui

local editors = {}
local img
local authors

function _init()
	local w = 150
	local h = 75
	if element_idx!=-1 then
		if element.type=="label" or element.type=="snippet" or element.type=="demo" then
			w=200
			h=125
		end
	else
		w=200
		h=100
	end
	gui = create_gui()
	if creating then
		w=125
		h=150
		local pull = gui:attach_pulldown({x=4,y=16,width_rel=1,width_add=-8,height_add=-34,height_rel=1})
		pull:attach_scrollbars({autohide=true})
		pull:attach_pulldown_item({
			label="Label",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="label",
					text="This is a label"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Note",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="note",
					text="This is a note"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Warning",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="warning",
					text="This is a warning"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Code snippet",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="snippet",
					text="--insert snippet here"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Demo",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="demo",
					text="function _draw()\n  cls(t()%5+8)\nend"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Embedded Pod Image",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="pod_image",
					data="b64:bHo0AEUAAABQAAAA9QdweHUAQyAQEAT-FAHUHgS8BB4EDBqMBgAAEADwBnwjDAQeBGxDBB4ELB8bDFMEHgQcPQYAIAxdFAAUvQQAUAQe1P4B"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Wiki page reference",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="wiki_ref",
					text="Go to landing page",
					ref="Landing"
				}})exit()
			end)
		})
		pull:attach_pulldown_item({
			label="Directory reference",action=(function(self)
				send_message(editor_pid,{event="attach_object",el={
					type="dir_ref",
					text="Open loaded cartridge",
					ref="/ram/cart"
				}})exit()
			end)
		})
	end
	window({
		width=w,height=h,title="Edit attributes",
		resizeable=false
	})
	gui:attach_button({
		x=w-44,y=h-15,width=40,height=13,
		label="Cancel",tap=(function(self)
			exit()
		end)
	})
	if not creating then
	gui:attach_button({
		x=w-88,y=h-15,width=40,height=13,
		label="Done",tap=(function(self)
			for ed in all(editors) do
				element[ed.parent.field] = table.concat(ed:get_text(),"\n")
			end
			if element_idx!=-1 then
				if element.type=="pod_image" then
					element.data = img.data
				end
			else
				element.version="0.4"
				element.authors = authors:get_text()
				send_message(editor_pid,{event="update_meta",data=element})
				exit()
			end
			send_message(editor_pid,{event="update_at",data={element=element,idx=element_idx}})
			exit()
		end)
	})
	end
	if not creating then
	if element.type=="label" or element.type=="note" or element.type=="warning" then
		local editor = gui:attach_text_editor({
			x=4,y=13,width=w-8,height=h-30,field="text"
		})
		add(editors,editor)
		editor:attach_scrollbars({autohide=true})
		editor:set_text(element.text)
		editor:set_keyboard_focus(true)
	end
	if element.type=="pod_image" then
		local gimg = gui:attach({
			x=4,y=13,width=w-8,height=h-30,data=element.data,
			draw=(function(self)
				rectfill(0,0,unpod(self.data):width()-1,unpod(self.data):height()-1,0)
				spr(unpod(self.data),0,0)
			end)
		})
		img = gimg
		local btn = gui:attach_button({
			x=4,y=h-15,width=40,height=13,gimg=gimg,
			label="Paste",tap=(function(self)
				local data = string.sub(get_clipboard(),27)
				data = string.sub(data,2,#data-2)
				if unpod(data) then
					self.gimg.data=data
				else
					notify("Copy a sprite from the GFX editor.")
				end
			end)
		})
	end
	if element.type=="snippet" or element.type=="demo" then
		local editor = gui:attach_text_editor({
			x=4,y=13,width=w-8,height=h-30,field="text",
			syntax_highlighting=true
		})
		add(editors,editor)
		editor:attach_scrollbars({autohide=true})
		editor:set_text(element.text)
		editor:set_keyboard_focus(true)
	end
	
	if element.type=="dir_ref" or element.type=="wiki_ref" then
		local editor = gui:attach_text_editor({
			x=4,y=13,width=w-8,height=13,field="text",
			max_lines=1,tap=(function(self)
				self.child[1]:set_keyboard_focus(true)
			end)
		})
		add(editors,editor)
		editor:set_text(element.text)
		editor:set_keyboard_focus(true)
		
		editor = gui:attach_text_editor({
			x=4,y=32,width=w-8,height=13,field="ref",
			max_lines=1,tap=(function(self)
				self.child[1]:set_keyboard_focus(true)
			end)
		})
		add(editors,editor)
		editor:set_text(element.ref)
	end
	
	if element_idx==-1 then
		local editor = gui:attach_text_editor({
			x=4,y=13,width=w-8,height=13,field="page_title",
			max_lines=1,tap=(function(self)
				self.child[1]:set_keyboard_focus(true)
			end)
		})
		add(editors,editor)
		editor:set_text(element.page_title)
		editor:set_keyboard_focus(true)
		
		authors = gui:attach_text_editor({
			x=4,y=30,width=w-8,height=50,
			tap=(function(self)
				self.child[1]:set_keyboard_focus(true)
			end)
		})
		authors:set_text(element.authors)
	end
	end
end

function _draw()
	cls(16)
	if element_idx!=-1 then
		?"Editing "..element.type,2,2,1
	else
		if not creating then
			?"Editing page metadata",2,2,1
		else
			?"Create new element...",2,2,1
		end
	end
	gui:update_all()
	gui:draw_all()
end
